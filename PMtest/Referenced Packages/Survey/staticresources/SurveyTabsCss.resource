@media (max-width:767px) {
    
.progress .circle {
	display: inline-block;
	margin: 0 auto 9px;
	min-width: 190px;
	padding: 5px;
	height: auto;
	min-height: 40px;
}
.progress .circle .label {
	float: left;
	border: 1px solid #d5d5da;
	margin: 0;
	line-height: 28px;
}
.progress .circle.active .label {
	border-color: transparent;
}
.progress .bar {
	width: 10px;
	height: 15px;
	top: 0!important;
	display: none;
}
.progress .circle .title {
	margin: 6px 0 0 0;
	width: 100%;
}
.progress {
	clear: both;
	margin: 15px 15px 0;
	text-align: center;
}
.progress .bar.done, .progress .circle.done {
	background: #fff;
}
}

@media (min-width:500px) and (max-width:767px) {
    
.progress .circle {
	width: 260px;
}
}

@media (min-width:500px) and (max-width:584px) {
.progress .circle {
	width: 208px;
}
}


@media (max-width:479px) {
.progress .circle {
	min-width: 100%!important;
	width: 100%!important;
}
}
