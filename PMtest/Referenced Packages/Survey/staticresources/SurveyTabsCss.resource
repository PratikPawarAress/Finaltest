 background:
 -webkit-linear-gradient(135deg, transparent, transparent 20%, rgba(0, 0, 0, 0.1) 33%, #45B565 50%, transparent 10%),  -webkit-linear-gradient(top, #81CE97, #81CE97),  -webkit-linear-gradient(left, #ba7448, #c4672d);
 background-size: 25px 14px, 100% 100%, 100% 100%;
 -webkit-animation: move 5s linear 0 infinite;
}
@-webkit-keyframes move {
 0% {
background-position: 0px 0px, 0 0, 0 0
