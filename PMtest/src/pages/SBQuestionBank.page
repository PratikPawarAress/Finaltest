.tooltip {
    position: relative;
    display: inline-block;
   
}

.tooltip .tooltiptext {
    visibility: hidden;
    width: 120px;
    background-color:#D6D6D6;
    color: black;
    text-align: center;
    border-radius: 6px;
    padding: 5px 0;

